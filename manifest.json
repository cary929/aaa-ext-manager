{
  "manifest_version": 3,
  "name": "AAA Extension Manager",
  "version": "1.0",
  "description": "管理 Chrome 扩展的简单工具",
  "permissions": [
    "management",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
